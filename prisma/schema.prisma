generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model TestResult {
  id         String   @id @default(uuid())
  createdAt  DateTime @default(now())
  testTitle  String
  score      Int
  maxScore   Int
  answers    String
  aiAnalysis String
  userEmail  String?
  userName   String?
}
